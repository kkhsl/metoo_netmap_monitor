<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.metoo.monitor.core.mapper.MetooVersionClientLogMapper">

    <select id="list" resultType="com.metoo.monitor.core.entity.MetooVersionClientLog"
            parameterType="com.metoo.monitor.core.vo.version.MetooVersionClientLogQueryVo">
        select * from metoo_version_client_log
        where 1=1
          and delete_STATUS=0
       <if test="resultFlag !=null and resultFlag !=''">
        and result_flag=#{resultFlag,jdbcType=TINYINT}
        </if>
    </select>

    <insert id="save" parameterType="com.metoo.monitor.core.entity.MetooVersionClientLog">
        insert into metoo_version_client_log
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="unitId != null">
                unit_id,
            </if>
            <if test="versionId != null">
                version_id,
            </if>
            <if test="version != null">
                version,
            </if>
            <if test="addTime != null">
                add_time,
            </if>
            <if test="opId != null">
                op_id,
            </if>
            <if test="opName != null">
                op_name,
            </if>
            <if test="versionStatus != null">
                version_status,
            </if>
            <if test="errorInfo != null">
                error_info,
            </if>
            <if test="deleteStatus != null">
                delete_status,
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">

            <if test="unitId != null">
                #{unitId,jdbcType=BIGINT},
            </if>
            <if test="versionId != null">
                #{versionId,jdbcType=BIGINT},
            </if>
            <if test="version != null">
                #{version,jdbcType=VARCHAR},
            </if>
            <if test="addTime != null">
                #{addTime,jdbcType=TIMESTAMP},
            </if>
            <if test="opId != null">
                #{opId,jdbcType=BIGINT},
            </if>
            <if test="opName != null">
                #{opName,jdbcType=VARCHAR},
            </if>
            <if test="versionStatus != null">
                #{versionStatus,jdbcType=TINYINT},
            </if>
            <if test="errorInfo != null">
                #{errorInfo,jdbcType=VARCHAR},
            </if>
            <if test="deleteStatus != null">
                #{deleteStatus,jdbcType=TINYINT},
            </if>
        </trim>
    </insert>
</mapper>
